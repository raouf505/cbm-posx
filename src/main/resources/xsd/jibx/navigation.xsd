<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://jibx.org/starter" elementFormDefault="qualified"
	targetNamespace="http://jibx.org/starter">

	<xs:element name="navigation" type="tns:navigation" />

	<xs:complexType name="navigation">
		<xs:annotation>
			<xs:documentation>Global Navigation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="link" type="tns:link" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="link">
		<xs:sequence>
			<xs:element name="id" type="xs:string" minOccurs="1" />
			<xs:element name="text" type="xs:string" minOccurs="1" />
			<xs:element name="href" type="xs:string" minOccurs="1" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>